/* pages/component/ads.wxss */
.ads-title-wrapper.common-padding {
  padding-bottom: 0;
  padding-right: 0;
}

.ads-title {
  font-size:13px;
  color:#323b45;
  padding-bottom: 13px;
  padding-right: 26px;
  border-bottom: 1px solid #f5f5f5;
}

.ads-swiper swiper-item .image-wrapper {
  padding-left: 5px;
  padding-right: 5px;
  height: 90px;
  text-align: right;
}

.ads-swiper swiper-item .image-wrapper > view {
  border: 1px solid #f5f5f5;
}

.ads-swiper swiper-item .image-wrapper image {
  width: 100%;
  height: 90px;
}

.ads-swiper swiper {
  height: 90px;
}

.ads-swiper {
  padding-top: 10px;
  padding-bottom: 20px;
}